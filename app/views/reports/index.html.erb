<h1>Listing reports</h1>

<table>
  <tr>
    <th>UPC</th>
    <th>Product</th>
    <th>Damage reason</th>
    <th>Employee Id</th>
    <th>picture 0 Uri</th>
    <th>picture 1 Uri</th>
    <th>signature Uri</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @reports.each do |report| %>
  <tr>
    <td><%= report.upc %></td>
    <td><%= report.productName %></td>
    <td><%= report.damageReason %></td>
    <td><%= report.employeeId %></td>
    <td><%= report.picture0Uri %></td>
    <td><%= report.picture1Uri %></td>
    <td><%= report.signatureUri %></td>
    <td><%= link_to 'Show', report %></td>
    <td><%= link_to 'Edit', edit_report_path(report) %></td>
    <td><%= link_to 'Destroy', report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Report', new_report_path %>
